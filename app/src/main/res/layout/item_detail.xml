<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="16dp">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/colorPrimary"
            android:text="@string/item_detail_nombre_usuario"
            />
        <TextView
            android:id="@+id/item_detail_username"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/colorText"
            android:text="@string/item_detail_nombre_usuario_example"
            android:layout_marginBottom="10dp"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/item_detail_apellido_usuario"
            android:textColor="@color/colorPrimary"
            />
        <TextView
            android:id="@+id/item_detail_lastname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/colorText"
            android:text="@string/item_detail_nombre_usuario_example"
            android:layout_marginBottom="10dp"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/item_detail_direccion_usuario"
            android:textColor="@color/colorPrimary"
            />

        <TextView
            android:id="@+id/item_detail_address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/colorText"
            android:text="@string/item_detail_nombre_usuario_example"
            android:layout_marginBottom="10dp"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/item_detail_dni_usuario"
            android:textColor="@color/colorPrimary"
            />

        <TextView
            android:id="@+id/item_detail_dni"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/colorText"
            android:text="@string/item_detail_nombre_usuario_example"
            android:layout_marginBottom="10dp"
            />
    </LinearLayout>


</LinearLayout>
